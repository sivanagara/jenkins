Pipline{
    agent any 
    stages {
        stage('Build') { 
            steps {
                echo"I am in build stage"
                }
        }
        stage('Test') { 
            steps {
                echo" I am in test stae" 
            }
        }
        stage('Deploy') { 
            steps {
                echo "I am in Deploy stage"
            }
        }
    }
}
